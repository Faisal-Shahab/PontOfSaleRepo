@{
    ViewData["Title"] = "Company Subscription";
}
 
<div class="row">
    <div class="col-md-6">
        <h4>List of Companies Subscriptions</h4>
    </div>
    <div class="col-md-6 text-right">
        <button id="add-new" class="btn btn-primary btn-sm">Add</button>
    </div>
</div>

<div class="row">
    <div class="col-md-12">
        <table class="table table-striped table-bordered" id="subscriptionDataTable">
            <thead>
                <tr>
                    <td>Id</td>
                    <td>Company</td>
                    <td>Subscription</td>
                    <td>Start</td>
                    <td>Expire</td>                    
                    <td>Actions</td>
                </tr>
            </thead>
        </table>
    </div>
</div>


<!--begin::Modal-->

<partial name="_modal">
    <!--end::Modal-->

    @section scripts{
        <script src="~/js/pages/companySubscription.js" asp-append-version="true"></script>
    }
